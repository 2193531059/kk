<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/sea_secret"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:background="@drawable/clickselector"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:layout_marginTop="2dp"
                android:layout_gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv1"
                    android:layout_centerVertical="true"
                    android:src="@drawable/sea_secret"
                    android:layout_width="25dp"
                    android:layout_height="25dp"/>
                <TextView
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/iv1"
                    android:layout_marginLeft="5dp"
                    android:text="@string/sea_secret"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>



                <ImageView
                    android:layout_centerVertical="true"
                    android:src="@drawable/btn_5"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/sea_zoology"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:background="@drawable/clickselector"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv2"
                    android:layout_centerVertical="true"
                    android:src="@drawable/sea_zoology"
                    android:layout_width="25dp"
                    android:layout_height="25dp"/>
                <TextView
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/iv2"
                    android:layout_marginLeft="5dp"
                    android:text="@string/sea_zoology"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <ImageView
                    android:layout_centerVertical="true"
                    android:src="@drawable/btn_5"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/sea_resource"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:background="@drawable/clickselector"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv3"
                    android:layout_centerVertical="true"
                    android:src="@drawable/sea_resouce"
                    android:layout_width="25dp"
                    android:layout_height="25dp"/>
                <TextView
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/iv3"
                    android:layout_marginLeft="5dp"
                    android:text="@string/sea_resource"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <ImageView
                    android:layout_centerVertical="true"
                    android:src="@drawable/btn_5"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/sea_defence"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:background="@drawable/clickselector"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv4"
                    android:layout_centerVertical="true"
                    android:src="@drawable/sea_defence"
                    android:layout_width="25dp"
                    android:layout_height="25dp"/>
                <TextView
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/iv4"
                    android:layout_marginLeft="5dp"
                    android:text="@string/sea_defence"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <ImageView
                    android:layout_centerVertical="true"
                    android:src="@drawable/btn_5"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ocean_sailing"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:background="@drawable/clickselector"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv5"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ocean_sailing"
                    android:layout_width="25dp"
                    android:layout_height="25dp"/>
                <TextView
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/iv5"
                    android:layout_marginLeft="5dp"
                    android:text="@string/ocean_sailing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <ImageView
                    android:layout_centerVertical="true"
                    android:src="@drawable/btn_5"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/coastal_tourism"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:background="@drawable/clickselector"
                android:paddingRight="15dp"
                android:paddingLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_gravity="center_vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/iv6"
                    android:layout_centerVertical="true"
                    android:src="@drawable/coastal_tourism"
                    android:layout_width="25dp"
                    android:layout_height="25dp"/>

                <TextView
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/iv6"
                    android:layout_marginLeft="5dp"
                    android:text="@string/coastal_tourism"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <ImageView
                    android:layout_centerVertical="true"
                    android:src="@drawable/btn_5"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>




</LinearLayout>
